{
  "name": "unixcrypt",
  "version": "1.1.0",
  "description": "Node.js implementation of Unixcrypt, specifically SHA-256 and SHA-512",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "clean": "rimraf dist",
    "prep": "npm install && npm run prep:husky",
    "prep:husky": "husky install && chmod ug+x .husky/*",
    "prepublishOnly": "npm run build",
    "test": "npm run clean && jest --coverage --verbose",
    "test:watch": "npm run test -- --watchAll",
    "report-coverage": "cat .coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markusberg/unixcrypt.git"
  },
  "keywords": [
    "sha256crypt",
    "sha512crypt",
    "unixcrypt"
  ],
  "author": "Markus Berg",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/markusberg/unixcrypt/issues"
  },
  "homepage": "https://github.com/markusberg/unixcrypt#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.19.0",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
